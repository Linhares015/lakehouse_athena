{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "python",
      "request": "launch",
      "name": "Run PySpark Notebook",
      "program": "${file}",
      "console": "integratedTerminal",
      "env": {
        "JAVA_HOME": "/usr/lib/jvm/java-17-openjdk-amd64",
        "SPARK_HOME": "${workspaceFolder}/spark",
        "PYTHONPATH": "${workspaceFolder}/spark/python:${workspaceFolder}/spark/python/lib/py4j-*.zip"
      }
    }
  ]
}
